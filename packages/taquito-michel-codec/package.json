{
  "name": "@exodus/taquito-michel-codec",
  "version": "16.1.1-exodus.10",
  "description": "Michelson parser/validator/formatter",
  "keywords": [
    "tezos",
    "blockchain",
    "signer"
  ],
  "main": "./dist/lib/taquito-michel-codec.js",
  "typings": "./dist/types/taquito-michel-codec.d.ts",
  "files": [
    "dist"
  ],
  "publishConfig": {
    "access": "public"
  },
  "author": "Eugene Zagidullin <eugene@ecadlabs.com>",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ecadlabs/taquito.git"
  },
  "license": "MIT",
  "engines": {
    "node": ">=16"
  },
  "scripts": {
    "test": "jest --coverage",
    "test:watch": "jest --coverage --watch",
    "test:prod": "npm run lint && npm run test -- --no-cache",
    "lint": "eslint --ext .js,.ts .",
    "precommit": "lint-staged",
    "prebuild": "rimraf dist",
    "version-stamp": "node ../taquito/version-stamping.js",
    "build": "tsc --project ./tsconfig.prod.json"
  },
  "lint-staged": {
    "{src,test}/**/*.ts": [
      "prettier --write",
      "eslint --fix",
      "git add"
    ]
  },
  "jest": {
    "transform": {
      ".(ts|tsx)": "ts-jest"
    },
    "testEnvironment": "node",
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(ts|tsx|js)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "moduleNameMapper": {
      "^@exodus/taquito-utils$": "<rootDir>/../taquito-utils/src/taquito-utils.ts"
    },
    "coveragePathIgnorePatterns": [
      "/node_modules/",
      "/test/"
    ],
    "collectCoverageFrom": [
      "src/**/*.{js,ts}"
    ]
  },
  "dependencies": {
    "base-x": "^4.0.0",
    "bs58check": "^2.1.2"
  },
  "devDependencies": {
    "@types/jest": "^29.5.5",
    "@types/node": "^20",
    "@typescript-eslint/eslint-plugin": "^5.28.0",
    "@typescript-eslint/parser": "^5.28.0",
    "eslint": "^8.17.0",
    "jest": "^29.7.0",
    "jest-config": "^29.7.0",
    "jest-extended": "^1.2.0",
    "lint-staged": "^13.0.1",
    "lodash.camelcase": "^4.3.0",
    "prettier": "^2.7.0",
    "rimraf": "^3.0.2",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.4.0",
    "ts-toolbelt": "^9.6.0",
    "typescript": "~4.9.5"
  },
  "gitHead": "551e35aeff7d6dcde1c72284238c0ed3c3aae77e",
  "type": "module"
}
