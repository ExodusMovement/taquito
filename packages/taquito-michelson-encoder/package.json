{
  "name": "@exodus/taquito-michelson-encoder",
  "version": "16.1.1-exodus.11",
  "description": "converts michelson data and types into convenient JS/TS objects",
  "keywords": [
    "tezos",
    "blockchain",
    "michelson",
    "smart-contract"
  ],
  "main": "dist/lib/taquito-michelson-encoder.js",
  "typings": "dist/types/taquito-michelson-encoder.d.ts",
  "files": [
    "dist"
  ],
  "publishConfig": {
    "access": "public"
  },
  "author": "Simon Boissonneault-Robert <simon@ecadlabs.com>",
  "repository": {
    "type": "git",
    "url": ""
  },
  "license": "MIT",
  "engines": {
    "node": ">=16"
  },
  "scripts": {
    "test": "exodus-test --jest --esbuild --coverage",
    "test:watch": "npm run test -- --watch",
    "test:prod": "npm run lint && npm run test -- --no-cache",
    "lint": "eslint --ext .js,.ts .",
    "precommit": "lint-staged",
    "prebuild": "rimraf dist",
    "version-stamp": "node ../taquito/version-stamping.js",
    "build": "tsc --project ./tsconfig.prod.json"
  },
  "lint-staged": {
    "{src,test}/**/*.ts": [
      "prettier --write",
      "eslint --fix",
      "git add"
    ]
  },
  "jest": {
    "transform": {
      ".(ts|tsx)": "ts-jest"
    },
    "testEnvironment": "node",
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(ts|tsx|js)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "coveragePathIgnorePatterns": [
      "/node_modules/",
      "/test/"
    ],
    "collectCoverageFrom": [
      "src/**/*.{js,ts}"
    ]
  },
  "dependencies": {
    "@exodus/taquito-rpc": "^16.1.1-exodus.11",
    "@exodus/taquito-utils": "^16.1.1-exodus.11",
    "bignumber.js": "^9.1.0",
    "fast-json-stable-stringify": "^2.1.0"
  },
  "devDependencies": {
    "@types/jest": "^29.5.5",
    "@types/node": "^20",
    "@typescript-eslint/eslint-plugin": "^5.28.0",
    "@typescript-eslint/parser": "^5.28.0",
    "colors": "^1.4.0",
    "coveralls": "^3.1.1",
    "cross-env": "^7.0.3",
    "eslint": "^8.17.0",
    "jest": "^29.7.0",
    "jest-config": "^29.7.0",
    "lint-staged": "^13.0.1",
    "lodash.camelcase": "^4.3.0",
    "prettier": "^2.7.0",
    "prompt": "^1.3.0",
    "replace-in-file": "^6.3.5",
    "rimraf": "^3.0.2",
    "shelljs": "^0.8.5",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.4.0",
    "ts-toolbelt": "^9.6.0",
    "typescript": "~4.9.5"
  },
  "gitHead": "551e35aeff7d6dcde1c72284238c0ed3c3aae77e",
  "type": "module"
}
