{
  "name": "@exodus/taquito-contracts-library",
  "version": "16.1.1-exodus.10",
  "description": "Can be used as an extension on the TezosToolkit to provide contracts data",
  "keywords": [
    "tezos"
  ],
  "main": "dist/lib/taquito-contracts-library.js",
  "typings": "dist/types/taquito-contracts-library.d.ts",
  "files": [
    "signature.json",
    "dist"
  ],
  "author": "Roxane Letourneau <roxane@ecadlabs.com>",
  "repository": {
    "type": "git",
    "url": ""
  },
  "publishConfig": {
    "access": "public"
  },
  "license": "MIT",
  "engines": {
    "node": ">=16"
  },
  "scripts": {
    "test": "jest --coverage",
    "test:watch": "jest --coverage --watch",
    "test:prod": "npm run lint && npm run test -- --no-cache",
    "lint": "eslint --ext .js,.ts .",
    "precommit": "lint-staged",
    "prebuild": "rimraf dist",
    "version-stamp": "node ../taquito/version-stamping.js",
    "build": "tsc --project ./tsconfig.prod.json "
  },
  "lint-staged": {
    "{src,test}/**/*.ts": [
      "prettier --write",
      "eslint --fix",
      "git add"
    ]
  },
  "jest": {
    "transform": {
      ".(ts|tsx)": "ts-jest"
    },
    "testEnvironment": "node",
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(ts|tsx|js)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "moduleNameMapper": {
      "^@exodus/taquito-taquito$": "<rootDir>/../taquito/src/taquito.ts",
      "^@exodus/taquito-rpc$": "<rootDir>/../taquito-rpc/src/taquito-rpc.ts",
      "^@exodus/taquito-taquito-utils$": "<rootDir>/../taquito-utils/src/taquito-utils.ts",
      "^@exodus/taquito-taquito-http-utils$": "<rootDir>/../taquito-http-utils/src/taquito-http-utils.ts"
    },
    "coveragePathIgnorePatterns": [
      "/node_modules/",
      "/test/"
    ],
    "collectCoverageFrom": [
      "src/**/*.{js,ts}"
    ]
  },
  "dependencies": {
    "@exodus/taquito-rpc": "^16.1.1-exodus.10",
    "@exodus/taquito-taquito": "^16.1.1-exodus.10",
    "@exodus/taquito-utils": "^16.1.1-exodus.10",
    "bignumber.js": "^9.1.0"
  },
  "devDependencies": {
    "@types/jest": "^29.5.5",
    "@types/node": "^20",
    "@types/superagent": "^4.1.19",
    "@typescript-eslint/eslint-plugin": "^5.28.0",
    "@typescript-eslint/parser": "^5.28.0",
    "colors": "^1.4.0",
    "coveralls": "^3.1.1",
    "cross-env": "^7.0.3",
    "eslint": "^8.17.0",
    "jest": "^29.7.0",
    "jest-config": "^29.7.0",
    "lint-staged": "^13.0.1",
    "lodash.camelcase": "^4.3.0",
    "prettier": "^2.7.0",
    "prompt": "^1.3.0",
    "replace-in-file": "^6.3.5",
    "rimraf": "^3.0.2",
    "shelljs": "^0.8.5",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.4.0",
    "ts-toolbelt": "^9.6.0",
    "typescript": "~4.9.5"
  },
  "gitHead": "551e35aeff7d6dcde1c72284238c0ed3c3aae77e",
  "type": "module"
}
